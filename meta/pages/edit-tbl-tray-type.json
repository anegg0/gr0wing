{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "type": "template-form",
                  "name": "form0",
                  "components": [
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Tray Name",
                              "component": "TrayName",
                              "name": "TrayNameLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 50,
                              "type": "textbox",
                              "name": "TrayName",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${tbltraytype.TrayName}"
                            },
                            {
                              "type": "required-validator",
                              "name": "TrayNameRequiredValidator",
                              "component": "TrayName",
                              "text": "TrayName is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Tray Handle",
                              "component": "TrayHandle",
                              "name": "TrayHandleLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 11,
                              "type": "textbox",
                              "name": "TrayHandle",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${tbltraytype.TrayHandle}"
                            },
                            {
                              "type": "required-validator",
                              "name": "TrayHandleRequiredValidator",
                              "component": "TrayHandle",
                              "text": "TrayHandle is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Tray Length",
                              "component": "TrayLength",
                              "name": "TrayLengthLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 11,
                              "type": "textbox",
                              "name": "TrayLength",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${tbltraytype.TrayLength}"
                            },
                            {
                              "type": "required-validator",
                              "name": "TrayLengthRequiredValidator",
                              "component": "TrayLength",
                              "text": "TrayLength is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Tray Width",
                              "component": "TrayWidth",
                              "name": "TrayWidthLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 11,
                              "type": "textbox",
                              "name": "TrayWidth",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${tbltraytype.TrayWidth}"
                            },
                            {
                              "type": "required-validator",
                              "name": "TrayWidthRequiredValidator",
                              "component": "TrayWidth",
                              "text": "TrayWidth is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Tray Height",
                              "component": "TrayHeight",
                              "name": "TrayHeightLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 11,
                              "type": "textbox",
                              "name": "TrayHeight",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${tbltraytype.TrayHeight}"
                            },
                            {
                              "type": "required-validator",
                              "name": "TrayHeightRequiredValidator",
                              "component": "TrayHeight",
                              "text": "TrayHeight is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to update TblTrayType",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "growing.updateTblTrayType",
                        "parameters": [
                          {
                            "name": "TrayTypeId",
                            "value": "${parameters.TrayTypeId}"
                          },
                          {
                            "name": "TblTrayType",
                            "value": "${tbltraytype}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${tbltraytype}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "itemType": "Localhost.Models.Growing.TblTrayType",
                  "data": "${tbltraytype}",
                  "visible": "${tbltraytype != null}"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "growing.getTblTrayTypeByTrayTypeId",
        "parameters": [
          {
            "name": "TrayTypeId",
            "value": "${parameters.TrayTypeId}"
          }
        ],
        "then": [
          {
            "name": "tbltraytype",
            "type": "set",
            "value": "${result}",
            "valueType": "Localhost.Models.Growing.TblTrayType"
          }
        ],
        "error": [],
        "type": "invoke"
      }
    ]
  },
  "name": "Edit Tbl Tray Type",
  "layout": "Main",
  "access": [
    "Authenticated"
  ]
}