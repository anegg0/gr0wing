                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to create new TblSeed!",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "growing.createTblSeed",
                        "parameters": [
                          {
                            "name": "TblSeed",
                            "value": "${tblseed}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${tblseed}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "data": "${tblseed}",
                  "visible": "${tblseed != null}",
                  "itemType": "Growing.Models.Growing.TblSeed"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "growing.getTblSeasons",
        "parameters": [],
        "then": [
          {
            "name": "getTblSeasonsForSeasonIdResult",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<Growing.Models.Growing.TblSeason>"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "tblseed",
        "type": "set",
        "value": "new Growing.Models.Growing.TblSeed(){}",
        "valueType": "Growing.Models.Growing.TblSeed"
      }
    ]
  },
  "name": "Add Tbl Seed",
  "layout": "Main",
  "access": [
    "Authenticated"
  ]
