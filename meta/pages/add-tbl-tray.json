{
  "access": [
    "Authenticated"
  ],
  "components": [
    {
      "components": [
        {
          "components": [
            {
              "class": "col-md-12",
              "components": [
                {
                  "components": [
                    {
                      "components": [
                        {
                          "class": "col-md-3",
                          "components": [
                            {
                              "component": "TrayTypeId",
                              "name": "TrayTypeIdLabel",
                              "style": {
                                "width": "100%"
                              },
                              "text": "Tbl Tray Type",
                              "type": "label"
                            }
                          ],
                          "type": "col"
                        },
                        {
                          "class": "col-md-9",
                          "components": [
                            {
                              "data": "${getTblTrayTypesForTrayTypeIdResult}",
                              "name": "TrayTypeId",
                              "placeholder": "Choose TblTrayType",
                              "style": {
                                "width": "100%"
                              },
                              "textProperty": "TrayName",
                              "type": "dropdown-datagrid",
                              "value": "${tbltray.TrayTypeId}",
                              "valueProperty": "TrayTypeId",
                              "valueType": "int?"
                            }
                          ],
                          "type": "col"
                        }
                      ],
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "type": "row"
                    },
                    {
                      "components": [
                        {
                          "class": "col-md-3",
                          "components": [
                            {
                              "component": "SeedId",
                              "name": "SeedIdLabel",
                              "style": {
                                "width": "100%"
                              },
                              "text": "Tbl Seed",
                              "type": "label"
                            }
                          ],
                          "type": "col"
                        },
                        {
                          "class": "col-md-9",
                          "components": [
                            {
                              "data": "${getTblSeedsForSeedIdResult}",
                              "name": "SeedId",
                              "placeholder": "Choose TblSeed",
                              "style": {
                                "width": "100%"
                              },
                              "textProperty": "SeedName",
                              "type": "dropdown-datagrid",
                              "value": "${tbltray.SeedId}",
                              "valueProperty": "SeedId",
                              "valueType": "int?"
                            }
                          ],
                          "type": "col"
                        }
                      ],
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "type": "row"
                    },
                    {
                      "components": [
                        {
                          "class": "col-md-3",
                          "components": [
                            {
                              "component": "CustomerId",
                              "name": "CustomerIdLabel",
                              "style": {
                                "width": "100%"
                              },
                              "text": "Tbl Customer",
                              "type": "label"
                            }
                          ],
                          "type": "col"
                        },
                        {
                          "class": "col-md-9",
                          "components": [
                            {
                              "data": "${getTblCustomersForCustomerIdResult}",
                              "name": "CustomerId",
                              "placeholder": "Choose TblCustomer",
                              "style": {
                                "width": "100%"
                              },
                              "textProperty": "CustomerName",
                              "type": "dropdown-datagrid",
                              "value": "${tbltray.CustomerId}",
                              "valueProperty": "CustomerId",
                              "valueType": "int?"
                            }
                          ],
                          "type": "col"
                        }
                      ],
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "type": "row"
                    },
                    {
                      "components": [
                        {
                          "class": "col-md-3",
                          "components": [
                            {
                              "component": "TraySequenceNumber",
                              "name": "TraySequenceNumberLabel",
                              "style": {
                                "width": "100%"
                              },
                              "text": "Tray Sequence Number",
                              "type": "label"
                            }
                          ],
                          "type": "col"
                        },
                        {
                          "class": "col-md-9",
                          "components": [
                            {
                              "name": "TraySequenceNumber",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "type": "numeric",
                              "value": "${tbltray.TraySequenceNumber}",
                              "valueType": "int"
                            },
                            {
                              "component": "TraySequenceNumber",
                              "name": "TraySequenceNumberRequiredValidator",
                              "style": {
                                "position": "absolute"
                              },
                              "text": "TraySequenceNumber is required",
                              "type": "required-validator"
                            }
                          ],
                          "type": "col"
                        }
                      ],
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "type": "row"
                    },
                    {
                      "components": [
                        {
                          "class": "col-md-3",
                          "components": [
                            {
                              "component": "TraySowingDate",
                              "name": "TraySowingDateLabel",
                              "style": {
                                "width": "100%"
                              },
                              "text": "Tray Sowing Date",
                              "type": "label"
                            }
                          ],
                          "type": "col"
                        },
                        {
                          "class": "col-md-9",
                          "components": [
                            {
                              "dateFormat": "MM/dd/yyyy",
                              "name": "TraySowingDate",
                              "style": {
                                "display": "block",
                                "float": "left",
                                "width": "203.5px"
                              },
                              "type": "datepicker",
                              "value": "${tbltray.TraySowingDate}",
                              "valueType": "DateTime",
                              "visible": true
                            }
                          ],
                          "type": "col"
                        }
                      ],
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "type": "row"
                    },
                    {
                      "components": [
                        {
                          "class": "col-md-3",
                          "components": [
                            {
                              "component": "TrayUpdateDate",
                              "name": "TrayUpdateDateLabel",
                              "style": {
                                "width": "100%"
                              },
                              "text": "Tray Update Date",
                              "type": "label"
                            }
                          ],
                          "type": "col"
                        },
                        {
                          "class": "col-md-9",
                          "components": [
                            {
                              "dateFormat": "MM/dd/yyyy",
                              "name": "TrayUpdateDate",
                              "style": {
                                "display": "block",
                                "float": "left",
                                "width": "203.5px"
                              },
                              "type": "datepicker",
                              "value": "${tbltray.TrayUpdateDate}",
                              "valueType": "DateTime"
                            }
                          ],
                          "type": "col"
                        }
                      ],
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "type": "row"
                    },
                    {
                      "components": [
                        {
                          "class": "col-md-3",
                          "components": [
                            {
                              "component": "ExpiryDate",
                              "name": "ExpiryDateLabel",
                              "style": {
                                "width": "100%"
                              },
                              "text": "Expiry Date",
                              "type": "label"
                            }
                          ],
                          "type": "col"
                        },
                        {
                          "class": "col-md-9",
                          "components": [
                            {
                              "dateFormat": "MM/dd/yyyy",
                              "name": "ExpiryDate",
                              "style": {
                                "width": "203.5px"
                              },
                              "type": "datepicker",
                              "value": "${tbltray.ExpiryDate}",
                              "valueType": "DateTime?"
                            }
                          ],
                          "style": {
                            "float": "none",
                            "textAlign": "justify"
                          },
                          "type": "col"
                        }
                      ],
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "type": "row"
                    },
                    {
                      "components": [
                        {
                          "class": "col-md-3",
                          "components": [
                            {
                              "component": "Status",
                              "name": "StatusLabel",
                              "style": {
                                "width": "100%"
                              },
                              "text": "Status",
                              "type": "label"
                            }
                          ],
                          "type": "col"
                        },
                        {
                          "class": "col-md-9",
                          "components": [
                            {
                              "maxLength": 50,
                              "name": "Status",
                              "style": {
                                "width": "100%"
                              },
                              "type": "textbox",
                              "value": "${tbltray.Status}",
                              "valueType": "string"
                            }
                          ],
                          "type": "col"
                        }
                      ],
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "type": "row"
                    },
                    {
                      "components": [
                        {
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "buttonStyle": "primary",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "type": "button"
                            },
                            {
                              "buttonStyle": "light",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              },
                              "style": {
                                "marginTop": "1px",
                                "width": "111.75px"
                              },
                              "text": "Cancel",
                              "type": "button"
                            }
                          ],
                          "type": "col"
                        }
                      ],
                      "type": "row"
                    }
                  ],
                  "data": "${tbltray}",
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to create new TblTray!",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "growing.createTblTray",
                        "parameters": [
                          {
                            "name": "TblTray",
                            "value": "${tbltray}"
                          }
                        ],
                        "then": [
                          {
                            "result": "${tbltray}",
                            "type": "close-popup"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "itemType": "Localhost.Models.Growing.TblTray",
                  "name": "form0",
                  "type": "template-form",
                  "visible": "${tbltray != null}"
                }
              ],
              "type": "col"
            }
          ],
          "type": "row"
        }
      ],
      "container": "main",
      "type": "content"
    }
  ],
  "events": {
    "load": [
      {
        "name": "growing.getTblTrayTypes",
        "parameters": [],
        "then": [
          {
            "name": "getTblTrayTypesForTrayTypeIdResult",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<Localhost.Models.Growing.TblTrayType>"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "growing.getTblSeeds",
        "parameters": [],
        "then": [
          {
            "name": "getTblSeedsForSeedIdResult",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<Localhost.Models.Growing.TblSeed>"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "growing.getTblCustomers",
        "parameters": [],
        "then": [
          {
            "name": "getTblCustomersForCustomerIdResult",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<Localhost.Models.Growing.TblCustomer>"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "tbltray",
        "type": "set",
        "value": "new Localhost.Models.Growing.TblTray(){}",
        "valueType": "Localhost.Models.Growing.TblTray"
      }
    ]
  },
  "layout": "Main",
  "name": "Add Tbl Tray"
}