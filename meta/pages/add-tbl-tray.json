{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "type": "template-form",
                  "name": "form0",
                  "components": [
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Tbl Customer",
                              "component": "CustomerId",
                              "name": "CustomerIdLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "data": "${getTblCustomersForCustomerIdResult}",
                              "textProperty": "CustomerContactName",
                              "valueProperty": "CustomerId",
                              "placeholder": "Choose TblCustomer",
                              "valueType": "int?",
                              "type": "dropdown-datagrid",
                              "name": "CustomerId",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${tbltray.CustomerId}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Expiry Date",
                              "component": "ExpiryDate",
                              "name": "ExpiryDateLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "DateTime?",
                              "dateFormat": "MM/dd/yyyy",
                              "type": "datepicker",
                              "name": "ExpiryDate",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${tbltray.ExpiryDate}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Tbl Seed",
                              "component": "SeedId",
                              "name": "SeedIdLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "data": "${getTblSeedsForSeedIdResult}",
                              "textProperty": "CurrentDTM",
                              "valueProperty": "SeedId",
                              "placeholder": "Choose TblSeed",
                              "valueType": "int?",
                              "type": "dropdown-datagrid",
                              "name": "SeedId",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${tbltray.SeedId}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Status",
                              "component": "Status",
                              "name": "StatusLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 50,
                              "type": "textbox",
                              "name": "Status",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${tbltray.Status}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Tray Sequence Number",
                              "component": "TraySequenceNumber",
                              "name": "TraySequenceNumberLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "int",
                              "type": "numeric",
                              "name": "TraySequenceNumber",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${tbltray.TraySequenceNumber}"
                            },
                            {
                              "type": "required-validator",
                              "name": "TraySequenceNumberRequiredValidator",
                              "component": "TraySequenceNumber",
                              "text": "TraySequenceNumber is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Tray Sowing Date",
                              "component": "TraySowingDate",
                              "name": "TraySowingDateLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "DateTime",
                              "dateFormat": "MM/dd/yyyy",
                              "type": "datepicker",
                              "name": "TraySowingDate",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${tbltray.TraySowingDate}"
                            },
                            {
                              "type": "required-validator",
                              "name": "TraySowingDateRequiredValidator",
                              "component": "TraySowingDate",
                              "text": "TraySowingDate is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Tbl Tray Type",
                              "component": "TrayTypeId",
                              "name": "TrayTypeIdLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "data": "${getTblTrayTypesForTrayTypeIdResult}",
                              "textProperty": "TrayHandle",
                              "valueProperty": "TrayTypeId",
                              "placeholder": "Choose TblTrayType",
                              "valueType": "int?",
                              "type": "dropdown-datagrid",
                              "name": "TrayTypeId",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${tbltray.TrayTypeId}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Tray Update Date",
                              "component": "TrayUpdateDate",
                              "name": "TrayUpdateDateLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "DateTime",
                              "dateFormat": "MM/dd/yyyy",
                              "type": "datepicker",
                              "name": "TrayUpdateDate",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${tbltray.TrayUpdateDate}"
                            },
                            {
                              "type": "required-validator",
                              "name": "TrayUpdateDateRequiredValidator",
                              "component": "TrayUpdateDate",
                              "text": "TrayUpdateDate is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to create new TblTray!",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "growing.createTblTray",
                        "parameters": [
                          {
                            "name": "TblTray",
                            "value": "${tbltray}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${tbltray}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "data": "${tbltray}",
                  "visible": "${tbltray != null}",
                  "itemType": "Growing.Models.Growing.TblTray"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "growing.getTblCustomers",
        "parameters": [],
        "then": [
          {
            "name": "getTblCustomersForCustomerIdResult",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<Growing.Models.Growing.TblCustomer>"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "growing.getTblSeeds",
        "parameters": [],
        "then": [
          {
            "name": "getTblSeedsForSeedIdResult",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<Growing.Models.Growing.TblSeed>"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "growing.getTblTrayTypes",
        "parameters": [],
        "then": [
          {
            "name": "getTblTrayTypesForTrayTypeIdResult",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<Growing.Models.Growing.TblTrayType>"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "tbltray",
        "type": "set",
        "value": "new Growing.Models.Growing.TblTray(){}",
        "valueType": "Growing.Models.Growing.TblTray"
      }
    ]
  },
  "name": "Add Tbl Tray",
  "layout": "Main",
  "access": [
    "Authenticated"
  ]
}