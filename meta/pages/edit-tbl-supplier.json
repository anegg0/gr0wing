                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Supplier Zipcode",
                              "component": "SupplierZipcode",
                              "name": "SupplierZipcodeLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 20,
                              "type": "textbox",
                              "name": "SupplierZipcode",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${tblsupplier.SupplierZipcode}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to update TblSupplier",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "growing.updateTblSupplier",
                        "parameters": [
                          {
                            "name": "SupplierId",
                            "value": "${parameters.SupplierId}"
                          },
                          {
                            "name": "TblSupplier",
                            "value": "${tblsupplier}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${tblsupplier}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "itemType": "Growing.Models.Growing.TblSupplier",
                  "data": "${tblsupplier}",
                  "visible": "${tblsupplier != null}"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "growing.getTblSupplierBySupplierId",
        "parameters": [
          {
            "name": "SupplierId",
            "value": "${parameters.SupplierId}"
          }
        ],
        "then": [
          {
            "name": "tblsupplier",
            "type": "set",
            "value": "${result}",
            "valueType": "Growing.Models.Growing.TblSupplier"
          }
        ],
        "error": [],
        "type": "invoke"
      }
    ]
  },
  "name": "Edit Tbl Supplier",
  "layout": "Main",
  "access": [
    "Authenticated"
  ]
}